    "function read_input()\n",
    "    strs = open(\"input3.txt\", \"r\") do f\n",
    "        readlines(f)\n",
    "    end\n",
    "    map(str -> from_string(str), strs)\n",
    "end\n",
    "\n",
    "function from_string(str::String)\n",
    "    chars = split(str, \"\")\n",
    "    return BitArray(parse(Bool, chars[i]) for i in 1:12)\n",
    "end"
   ]
